{
  "name": "api.cccvcommunity.org",
  "private": true,
  "version": "1.0.0",
  "description": "API for CCCV Community.",
  "main": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "test:unit": "./node_modules/.bin/jest unit",
    "test:unit:coverage": "./node_modules/.bin/jest unit --coverage",
    "test:int": "./node_modules/.bin/jest int",
    "test:int:coverage": "./node_modules/.bin/jest int --coverage",
    "copy:graphql": "./node_modules/.bin/cpx src/**/*.gql build -w",
    "build:watch": "./node_modules/.bin/tsc --watch",
    "start:watch": "./node_modules/.bin/nodemon build/index.js",
    "watch": "npm run build && ./node_modules/.bin/concurrently -k \"npm:build:watch\" \"npm:start:watch\" \"npm:copy:graphql\"",
    "build": "./node_modules/.bin/rimraf ./build && ./node_modules/.bin/cpx src/**/*.gql build && ./node_modules/.bin/tsc",
    "heroku-postbuild": "npm run build",
    "start": "node build/index.js",
    "lint": "./node_modules/.bin/tslint --project tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cccvcommunity/api.cccvcommunity.org.git"
  },
  "author": "BenedictOng42",
  "license": "Apache-2.0",
  "dependencies": {
    "@okgrow/graphql-scalars": "0.4.2",
    "apollo-server-express": "2.2.6",
    "axios": "0.18.0",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-sslify": "1.2.0",
    "graphql": "14.0.2",
    "graphql-iso-date": "3.6.1",
    "graphql-type-json": "0.2.1",
    "https": "1.0.0",
    "joi": "14.3.0",
    "jsonwebtoken": "8.4.0",
    "moment": "2.23.0",
    "moment-timezone": "0.5.23",
    "mongodb": "3.1.10",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.0.1",
    "@commitlint/prompt-cli": "7.0.0",
    "@types/chalk": "2.2.0",
    "@types/compression": "0.0.36",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/graphql": "14.0.3",
    "@types/graphql-iso-date": "3.3.1",
    "@types/jest": "23.3.11",
    "@types/joi": "14.0.1",
    "@types/jsonwebtoken": "8.3.0",
    "@types/moment-timezone": "0.5.9",
    "@types/mongodb": "3.1.17",
    "@types/morgan": "1.7.35",
    "@types/sinon": "7.0.3",
    "concurrently": "3.6.0",
    "cpx": "1.5.0",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "nodemon": "1.18.7",
    "rimraf": "2.6.2",
    "sinon": "7.2.2",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "typescript": "3.2.2"
  }
}
